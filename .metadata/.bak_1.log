		interpolatedColor[2] = (short) Math.max(0, Math.min(255, (p1_color[2] * barycentricCoordinates.get(0)
				+ p2_color[2] * barycentricCoordinates.get(1) + p3_color[2] * barycentricCoordinates.get(2))));
