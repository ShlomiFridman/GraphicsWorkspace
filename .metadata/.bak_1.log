				y1 = Math.round(p2.y);
				x2 = Math.round(p1.x);
				y2 = Math.round(p1.y);
			}

			dx = x2 - x1;
			dy = y2 - y1;
			if (Math.abs(dx) > Math.abs(dy)) {
				diff = 2 * dy - dx;
				yIncrement = 1;
				if (dy < 0) {
					yIncrement = -1;
					dy = -dy;
				}
				y = y1;
				for (x = x1; x < x2; x++) {
					intBufferWrapper.setPixel(x, y, r, g, b);
					if (diff < 0) {
						diff += 2 * dy;
					} else {
						y += yIncrement;
						diff += 2 * dy - 2 * dx;
					}
				}
			} else {
				diff = 2 * dx - dy;
				xIncrement = 1;
				if (dx < 0) {
					xIncrement = -1;
					dx = -dx;
				}
				x = x1;
