			int dx, dy, x, y, x1, y1, x2, y2, diff, yIncrement, xIncrement;

			if ((p2.y - p1.y) > -(p2.x - p1.x)) {
				x1 = Math.round(p1.x);
				y1 = Math.round(p1.y);
