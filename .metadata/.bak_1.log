	private Vector3f lineFrom2Points(Vector3f p1, Vector3f p2) {
		return new Vector3f(p2.y - p1.y, -(p2.x - p1.x), p1.y * (p2.x - p1.x) - p1.x * (p2.y - p1.y));
	}

	private float distanceOfPointFromLine(Vector3f line, Vector2f p) {
