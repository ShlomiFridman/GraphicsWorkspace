			int x1, y1, x2, y2;
			if ((p2.y - p1.y) > -(p2.x - p1.x)) {
				x1 = (int) p1.x;
				y1 = (int) p1.y;
				x2 = (int) p2.x;
				y2 = (int) p2.y;
